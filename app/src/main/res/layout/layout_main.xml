<?xml version="1.0" encoding="utf-8"?>
<com.sunc.cube.view.PanelLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_panel"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:layout_editor_absoluteY="8dp"
    tools:layout_editor_absoluteX="8dp"
    android:layout_alignParentBottom="true"
    android:layout_centerHorizontal="true"
    android:background="@drawable/bg_panel">
    <ImageView
        android:id="@+id/iv_img0"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:tag="0"
        android:scaleType="fitXY"/>
    <ImageView
        android:id="@+id/iv_img1"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:layout_marginTop="@dimen/image_piece_size"
        android:tag="1"
        android:scaleType="fitXY"/>
    <ImageView
        android:id="@+id/iv_img2"
        android:layout_marginTop="@dimen/image_piece_size"
        android:layout_marginLeft="@dimen/image_piece_size"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:tag="2"
        android:scaleType="fitXY"/>
    <ImageView
        android:id="@+id/iv_img3"
        android:layout_marginTop="@dimen/image_piece_size"
        android:layout_marginLeft="200dp"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:tag="3"
        android:scaleType="fitXY"/>
    <ImageView
        android:id="@+id/iv_img4"
        android:layout_marginTop="200dp"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:tag="4"
        android:scaleType="fitXY"/>
    <ImageView
        android:id="@+id/iv_img5"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="@dimen/image_piece_size"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:tag="5"
        android:scaleType="fitXY"/>
    <ImageView
        android:id="@+id/iv_img6"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="200dp"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:tag="6"
        android:scaleType="fitXY"/>
    <ImageView
        android:id="@+id/iv_img7"
        android:layout_marginTop="300dp"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:tag="7"
        android:scaleType="fitXY"
        />
    <ImageView
        android:id="@+id/iv_img8"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="@dimen/image_piece_size"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:tag="8"
        android:scaleType="fitXY"/>
    <ImageView
        android:id="@+id/iv_img9"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="200dp"
        android:layout_width="@dimen/image_piece_size"
        android:layout_height="@dimen/image_piece_size"
        android:tag="9"
        android:scaleType="fitXY"/>
</com.sunc.cube.view.PanelLayout>
